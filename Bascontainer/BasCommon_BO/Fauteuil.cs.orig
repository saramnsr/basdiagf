using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace BasCommon_BO
{
    [Serializable]
    public class Fauteuil
    {
        public override string ToString()
        {
            return this.Name;
        }

        public int Id;
        public string libelle
        {
            get
            {
                return this.Name;
            }
            set
            {
                this.Name = value;
            }
        }

        private string m_Name;
        public string Name
        {
            get
            {
                return m_Name;
            }
            set
            {
                m_Name = value;

            }
        }



    }
}
